{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",        
    "parameters": {
        "ParentResourceGroupName": {
            "value": "ccafpi"
          
        },
        "uniqueDnsNameForTM": {            
            "value": "ccafpi-tm"            
        },
        "keyvault_name": {
            "value": "d365ccafpi"            
        },
        "keysAppPermissions": {            
            "value": [
                "list",
                "get"
            ]            
        },
        "secretsAppPermissions": {           
            "value": [
                "list",
                "get"
            ]            
        },
        "certificatesAppPermissions": {           
            "value": [
                "list",
                "get"
            ]
        },
        "keysUserPermissions": {
			"value": [
				"Get",
				"List",
				"Update",
				"Create",
				"Import",
				"Delete",
				"Recover",
				"Backup",
				"Restore"
			]
        },
        "secretsUserPermissions": {            
            "value": [
                "Get",
                "List",
                "Set",
                "Delete",
                "Recover",
                "Backup",
                "Restore"
            ]
        },
        "certificatesUserPermissions": {           
            "value": [
                "Get",
                "List",
                "Update",
                "Create",
                "Import",
                "Delete",
                "Recover",
                "Backup",
                "Restore",
                "ManageContacts",
                "ManageIssuers",
                "GetIssuers",
                "ListIssuers",
                "SetIssuers",
                "DeleteIssuers"
            ]
        },
        "SSLdomainName": {
            "value": "www.d365ccafpi.com"            
        },
        "appServicesPerRegions": {
            "type": "array",
            "value": [
                {
                    "location": "West US",
                    "appName": "prod-amer-ccafpi-webapp-0",
                    "resourceGroupName": "prod-amer-ccafpi-rg-0"
                },
                {
                    "location": "East US",
                    "appName": "prod-amer-ccafpi-webapp-1",
                    "resourceGroupName": "prod-amer-ccafpi-rg-1"
                }
            ]
        },
        "msdeployPackageUrl": {          
            "value": "SampleAPI.zip"
        }
    }
}
